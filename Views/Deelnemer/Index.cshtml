@model IEnumerable<SamenGamen.Models.Deelnemer>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Naam)
        </th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Naam)
            </td>
            <td>
                <a asp-action="StartSessie" asp-controller="Deelnemer" asp-route-id="@item.Id">Aanmelden voor games</a>
            </td>
        </tr>
    }
    </tbody>
</table>